/*---------------------------------------------------------------------------*/
/*  QSlider  滑动条  */

QSlider::groove:horizontal{
background:#404244;
height:8px;
border-radius:4px;
}

QSlider::add-page:horizontal{
background:#404244;
height:8px;
border-radius:4px;
}

QSlider::sub-page:horizontal{
background:#67696B;
height:8px;
border-radius:4px;
}

QSlider::handle:horizontal{
width:13px;
margin-top:-3px;
margin-bottom:-3px;
border-radius:6px;
background:qradialgradient(
spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #2E2F30,stop:0.8 #67696B);
}

QSlider::groove:vertical{
width:8px;
border-radius:4px;
background:#404244;
}

QSlider::add-page:vertical{
width:8px;
border-radius:4px;
background:#404244;
}

QSlider::sub-page:vertical{
width:8px;
border-radius:4px;
background:#67696B;
}

QSlider::handle:vertical{
height:14px;
margin-left:-3px;
margin-right:-3px;
border-radius:6px;
background:qradialgradient(
spread:pad,cx:0.5,cy:0.5,radius:0.5,fx:0.5,fy:0.5,stop:0.6 #2E2F30,stop:0.8 #67696B);
}

/*---------------------------------------------------------------------------*/
/*  QScrollBar  滑动块  */
QScrollBar:vertical {
background:rgb(81, 81, 86);
margin:20px 0px;
width:20px;
font-size:14px;
}
QScrollBar::handle:vertical {
border-radius: 3px;
border:1px solid rgb(74, 74, 80);
background: rgb(114, 114, 119);
}
QScrollBar::handle:vertical:hover {
background: rgb(137, 137, 141);
}
QScrollBar::handle:vertical:pressed {
background: rgb(101, 101, 107);
}
QScrollBar::add-line:vertical {
height:20px;
subcontrol-position: bottom;
subcontrol-origin: margin;
background: url(:/Style/StyleResources/scrollbardown01.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::add-line:vertical:hover {
background: url(:/Style/StyleResources/scrollbardown02.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::sub-line:vertical {
height:20px;
subcontrol-position: top;
subcontrol-origin: margin;
background: url(:/Style/StyleResources/scrollbarup01.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::sub-line:vertical:hover {
background: url(:/Style/StyleResources/scrollbarup02.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
background: none;
}
QScrollBar:horizontal {
background:rgb(81, 81, 86);
margin:0px 20px;
height:20px;
font-size:14px;
}
QScrollBar::handle:horizontal {
border-radius: 3px;
border:1px solid rgb(74, 74, 80);
background: rgb(114, 114, 119);
}
QScrollBar::handle:horizontal:hover {
background: rgb(137, 137, 141);
}
QScrollBar::handle:horizontal:pressed {
background: rgb(101, 101, 107);
}
QScrollBar::add-line:horizontal {
width:20px;
subcontrol-position: right;
subcontrol-origin: margin;
background: url(:/Style/StyleResources/scrollbarright01.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::add-line:horizontal:hover {
background: url(:/Style/StyleResources/scrollbarright02.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::sub-line:horizontal {
width:20px;
subcontrol-position: left;
subcontrol-origin: margin;
background: url(:/Style/StyleResources/scrollbarleft01.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::sub-line:horizontal:hover {
background: url(:/Style/StyleResources/scrollbarleft02.png);
background-color:rgb(81, 81, 86);
}
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
background: none;
}


