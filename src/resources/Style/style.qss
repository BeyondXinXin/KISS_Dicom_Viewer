background: rgba(33, 36, 38, 255);
border:1px solid rgba(47, 51, 54, 255);
color: rgba(80, 87, 92, 1);
width: 100px;
height: 30px;
}
QTabBar::tab:hover {
background: rgba(37, 41, 43, 255);
}
QTabBar::tab:selected {
color: rgba(184, 200, 212, 1);
background: rgba(37, 41, 43, 255);
}

/*---------------------------------------------------------------------------*/
/*  QStatusBar  状态栏  */

QStatusBar::item{
border:0px solid #404244;
border-radius:3px;
}

/*---------------------------------------------------------------------------*/
/*  QToolTip  显示提示  */

QToolTip{
border:0px solid #BEC0C2;
padding:1px;
color:#BEC0C2;
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #404244,stop:1 #404244);
}

/*---------------------------------------------------------------------------*/
/*  QToolBox  工具盒类  */

QToolBox::tab:selected{
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #262829,stop:1 #262829);
}

/*---------------------------------------------------------------------------*/
/*  QPrintPreviewDialog  打印预览对话框  */

QPrintPreviewDialog QToolButton{
border:0px solid #BEC0C2;
border-radius:0px;
margin:0px;
padding:3px;
background:none;
}

/*---------------------------------------------------------------------------*/
/*  QColorDialog  颜色对话框  */
QColorDialog QPushButton{
min-width:80px;
}

/*---------------------------------------------------------------------------*/
/*  QFileDialog  文件对话框  */
QFileDialog QPushButton{
    min-width:80px;
}
/*---------------------------------------------------------------------------*/
/*  QInputDialog    */
QInputDialog QPushButton{
    min-width:80px;
}

/*---------------------------------------------------------------------------*/
/*  QCalendarWidget  日历对话框  */
QCalendarWidget QToolButton::menu-indicator{
    image:None;
}

/*---------------------------------------------------------------------------*/
/*  QCalendarWidget  */
QCalendarWidget QTableView{
    border-width:0px;
}

/*---------------------------------------------------------------------------*/
/*  QTableView  */

QTableView{
border:1px solid #67696B;
selection-background-color:#262829;
selection-color:#BEC0C2;
alternate-background-color:#262829;
gridline-color:#67696B;
}

QTableView{
background:#2E2F30;
}

QTableView::item:selected{
color:#BEC0C2;
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #404244,stop:1 #404244);
}

QTableView::item:hover{
color:#BEC0C2;
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #262829,stop:1 #262829);
}

QTableView::item{
padding:1px;
margin:0px;
}

QTableView QLineEdit,
QTableView QComboBox,
QTableView QSpinBox,
QTableView QDoubleSpinBox,
QTableView QDateEdit,
QTableView QTimeEdit,
QTableView QDateTimeEdit{
border-width:0px;
border-radius:0px;
}

QTableView QLineEdit:focus,
QTableView QComboBox:focus,
QTableView QSpinBox:focus,
QTableView QDoubleSpinBox:focus,
QTableView QDateEdit:focus,
QTableView QTimeEdit:focus,
QTableView QDateTimeEdit:focus{
border-width:0px;
border-radius:0px;
}

/*---------------------------------------------------------------------------*/
/*  QTabWidget  表格视窗  */

QTabWidget::pane {
background-color:rgba(37, 41, 43, 1);
border:1px solid rgba(47, 51, 54, 1);
}
QTabWidget::pane:hover {
background-color:rgba(37, 41, 43, 1);
border:1px solid rgba(47, 51, 54, 1);
}
QTabWidget {
background-color:rgba(37, 41, 43, 1);
border:1px solid rgba(47, 51, 54, 1);
}


/*---------------------------------------------------------------------------*/
/*  QStackedWidget  切换窗口  */
QStackedWidget{

}

/*---------------------------------------------------------------------------*/
/*  QGraphicsView  图形窗口  */
QGraphicsView{
background-color:rgb(90, 90, 90);
border:0px
}


